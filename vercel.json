{
  "version": 2,
  "builds": [
    {
      "src": "app/main.py",
      "use": "@vercel/python",
      "config": { "runtime": "python@3.9" }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",   
      "dest": "app/main.py"
    },
    {
      "src": "/(.*)",       
      "dest": "app/main.py" 
    }
  ]
}